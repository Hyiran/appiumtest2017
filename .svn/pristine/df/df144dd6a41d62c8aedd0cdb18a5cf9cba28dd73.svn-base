package com.oribo.ReadExcelCase;
/**
 * Excel 表对象，数据封装，无方法（第几张表，起始及结束位置）
 * @author Administrator
 *
 */
public class ExcelData {
	int numerSheet;//读取第几张表  小于 0 读取整个表，大于= 0 读取指定表
	int firstIndex = 1;//读取数据第几个开始,默认第二行开始
	int lastIndex =0;//读取一张表的时候结束数据位置
    String caseType;
	public int getNumerSheet() {
		return numerSheet;
	}
	public void setNumerSheet(int numerSheet) {
		this.numerSheet = numerSheet;
	}
	public int getFirstIndex() {
		return firstIndex;
	}
	public void setFirstIndex(int firstIndex) {
		this.firstIndex = firstIndex;
	}
	public int getLastIndex() {
		return lastIndex;
	}
	public String getCaseType() {
		return caseType;
	}
	public void setCaseType(String caseType) {
		this.caseType = caseType;
	}
	public void setLastIndex(int lastIndex) {
		this.lastIndex = lastIndex;
	}
	
}
