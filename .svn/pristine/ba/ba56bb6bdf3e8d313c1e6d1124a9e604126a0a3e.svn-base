			Assert.assertNotNull(Driver.getAndroidDriver(null).findElementByName("我的主机"));
			AndroidElement  	HostName = Driver.getAndroidDriver(null).findElementByName("我的主机");
			if(null!= HostName) {
				newAssertEquals(Driver.getAndroidDriver(null), data ,data.getExpectValue() , mLogger, null);
