	/**
	 * 
	 * @param 删除主机
	 */
	private boolean  DeleteHost(int index) {
		DataBean  data = bean.get(index);
		LoggerUtil.startTest(data.getTestCaseName(), mLogger);
		try {				
			AndroidElement  	HostName = Driver.getAndroidDriver(null).findElementById("com.orvibo.homemate:id/deleteTextView");//更多页面检查删除主机
			newSleep(1);
			if(HostName.getText().equals("删除主机")) {
				HostName.click();//点击删除主机
				newSleep(1);
			}
					
			AndroidElement  	DeleteButton = Driver.getAndroidDriver(null).findElementById("com.orvibo.homemate:id/leftButton");//删除按钮			
			newSleep(1);
			if(DeleteButton.getText().equals("删除")) {			
				currentActivity = Driver.getAndroidDriver(null).currentActivity();
				DeleteButton.click();//点击删除，返回设备页面
				newSleep(2);
				AndroidElement  	BackPageName = Driver.getAndroidDriver(null).findElementByName("家");
			if (BackPageName.getText().equals("家")) {
					System.out.println("删除主机成功");
					newAssertEquals(Driver.getAndroidDriver(null), data ,data.getExpectValue(), mLogger, null);
				}			
				else if(Driver.getAndroidDriver(null).findElementByName("更多").getText().equals("更多")){
					System.out.println("删除主机失败");
					newAssertEquals(Driver.getAndroidDriver(null), data ,data.getActualValue() , mLogger, null);
				}
					
				
				} else{
					System.out.println("没有找到删除按钮："+DeleteButton.getText());
					
				}
				
		} catch (Exception e) {
			newAssertEquals(Driver.getAndroidDriver(null), data ,e.getMessage(), mLogger, e.getMessage());		  ;;;;;;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
			
		}

		LoggerUtil.endTest(data.getTestCaseName(), mLogger);
		return true;
	}
	
